// Request the receiver to output a log message. The message may be ignored if
// the receiver's configured loglevel is lower than the message's loglevel.

#version 4
#version 5
#version 6

enum ofp_bsn_loglevel(wire_type=uint8_t) {
    OFP_BSN_LOGLEVEL_MSG = 0,
    OFP_BSN_LOGLEVEL_ERROR = 1,
    OFP_BSN_LOGLEVEL_WARN = 2,
    OFP_BSN_LOGLEVEL_INFO = 3,
    OFP_BSN_LOGLEVEL_VERBOSE = 4,
    OFP_BSN_LOGLEVEL_TRACE = 5,
};

struct of_bsn_log : of_bsn_header {
    uint8_t version;
    uint8_t type == 4;
    uint16_t length;
    uint32_t xid;
    uint32_t experimenter == 0x5c16c7;
    uint32_t subtype == 63;
    enum ofp_bsn_loglevel loglevel;
    of_octets_t data; /* US-ASCII string, no null terminator */
};
